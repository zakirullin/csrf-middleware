{
  "name": "zakirullin/csrf-middleware",
  "description": "PSR-15 middleware to handle CSRF-token verification",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Artem Zakirullin",
      "email": "artem.zakirullin@gmail.com"
    }
  ],
  "keywords": [
    "csrf",
    "psr-15",
    "psr-7",
    "middleware",
    "server",
    "http",
    "token"
  ],
  "homepage": "https://github.com/zakirullin/csrf-middleware",
  "require": {
    "php": "^7.1",
    "psr/http-message": "^1.0.1",
    "psr/http-server-middleware": "^1.0",
    "middlewares/utils": "^2.1.1",
    "zendframework/zend-diactoros": "^1.7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Zakirullin\\Middlewares\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zakirullin\\Middlewares\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "phpunit --stderr --coverage-text"
    ],
    "coverage": "phpunit --coverage-html=coverage"
  }
}
